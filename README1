

-- Comando para rodar no banco postgres


DELETE FROM pedidos_retirada; -- Apaga dados da tabela
ALTER SEQUENCE pedidos_retirada_id_seq RESTART WITH 1; -- reseta o id para 1


-- Limpa os dados e reinicia os IDs (sem excluir as tabelas)
TRUNCATE TABLE
  historico_status,
  mensagens_pedido,
  notificacoes_lidas,
  pedidos_entrega,
  pedidos_pronta_entrega,
  pedidos_retirada,
  produtos,
  responsavel_producao
RESTART IDENTITY;

